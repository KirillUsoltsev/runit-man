<% content_for :content do %>
    <div id="error" class="error">
        <%= h(t.runit.error('<span id="url"></span>')) %>.
    </div>
    <div id="services">
        <%= t.runit.loading %>
    </div>
<% end %>
<% content_for :footer do %>
    <p>
        <%= t.runit.footer('<span id="service-refresh-interval">?</span>') %>
    </p>
<% end %>
<% @scripts << 'runit-man' %>
